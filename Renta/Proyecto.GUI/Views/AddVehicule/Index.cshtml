@model IEnumerable<Proyecto.GUI.Models.AddVehicule>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SelectedModelo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SelectedMecanico)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SelectedProveedor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Precio)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SelectedModelo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SelectedMecanico)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SelectedProveedor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Precio)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
