<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav">
                <li class="nav-item">
                    @Html.ActionLink("Carros", "Index", "Home")
                </li>
                <li class="nav-item">
                    @Html.ActionLink("Clientes", "Index", "Cliente")
                </li>
                <li class="nav-item">
                    @Html.ActionLink("Empleados", "Index", "Empleado")
                </li>
                <li class="nav-item">
                    @Html.ActionLink("Proveedor", "Index", "Proveedor")
                </li>
                <li class="nav-item">
                    @Html.ActionLink("Mecánico", "Index", "Mecanico")
                </li>
                <li class="nav-item">
                    @Html.ActionLink("Facturas", "Index", "Facturas")
                </li>
            </ul>
        </div>
    </nav>
    <div class="container body-content">
        @RenderBody()

    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>


